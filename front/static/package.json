  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "format": "npx prettier --write .",
    "prepare": "husky install || echo 'Husky skipped in Docker build'"
